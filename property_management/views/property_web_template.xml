<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="web_form_template">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty">
<!--                <t t-out="Details"/>Details-->
                <table>
                    <thead>
                        <tr align="center">
                            <th colspan="6" class="text-center">Sl.no</th>
                            <th colspan="6" class="text-center">Sequence.no</th>
                            <th colspan="6" class="text-center">Property</th>
                            <th colspan="6" class="text-center">Type</th>
                            <th colspan="6" class="text-center">Tenant name</th>
                            <th colspan="6" class="text-center">Start date</th>
                            <th colspan="6" class="text-center">End date</th>
                            <th colspan="6" class="text-center">Rent/Lease Amount</th>
                            <th colspan="6" class="text-center">State</th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-set="i" t-value="1"/>
                        <t t-foreach="rent_details" t-as="property">
                            <tr>
                                <td colspan="6">
                                    <span t-esc="i"/>
                                    <t t-set="i" t-value="i+1"/>
                                </td>
                                <td colspan="6">
                                    <a t-attf-href="/web_details/{{property.id}}">
                                        <span t-esc="property.sequence_no"/>
                                    </a>
                                </td>
                                <td colspan="6">
                                    <span t-esc="property.property_ids"/>
                                </td>
                                <td colspan="6">
                                    <span t-esc="property.type"/>
                                </td>
                                <td colspan="6">
                                    <span t-esc="property.tenant_id"/>
                                </td>

                                <td colspan="6">
                                    <span t-esc="property.start_date"/>
                                </td>
                                <td colspan="6">
                                    <span t-esc="property.end_date"/>
                                </td>
                                <td colspan="6">
                                    <span t-esc="property.rent_lease_amount"/>
                                </td>
                                <td colspan="6">
                                    <span t-esc="property.rent_states"/>
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>
                <button><a t-attf-href="/new_form">Create</a></button>
            </div>
        </t>
    </template>
</odoo>