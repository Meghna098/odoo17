<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="new_web_form_template">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty">
                <div class="container">
                    <form action="action_page">
                        <div>
                            <div>
                                <label for="name">Property</label>
<!--                                <input type="text" id="name" name="property"/>-->
                                <select name="property" id="property"
                                        style="width:290px; height:45px"
                                        class="advanced-select">
                                        <t t-foreach='property' t-as="field">
                                            <option t-esc='field.property' t-att-value="field.id"/>
                                        </t>
                                </select>
                            </div>
                        </div>
                        <div>
                            <div>
                                <label for="name">Type</label>
                                <input type="text" id="phone" name="type"/>
                            </div>
                        </div>
                        <div>
                            <div>
                                <label for="phone">Start date</label>
                                <input type="phone" id="phone" name="phone"/>
                            </div>
                        </div>
                        <div>
                            <div>
                                <label for="email">End date</label>
                                <input type="email" id="email" name="email"/>
                            </div>
                        </div>
                        <div>
                            <button class="btn-submit">Submit</button>
                        </div>
                    </form>
                </div>
            </div>
        </t>
    </template>
</odoo>